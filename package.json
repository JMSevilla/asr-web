{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --experimental-https",
    "build": "next build",
    "start": "node --require dd-trace/init node_modules/.bin/next start",
    "lint": "next lint",
    "format": "prettier '**/*.ts*' --write",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "node --expose-gc ./node_modules/.bin/jest --coverage --logHeapUsage -w=30% --silent --testResultsProcessor jest-sonar-reporter",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "clear_jest": "jest --clearCache",
    "test:ci": "jest --ci",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start 3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev 3000 cy:open",
    "audit": "audit-ci --critical --report-type important"
  },
